{
    "name": "ecfectus/application",
    "description": "The Ecfectus Application",
    "license": "MIT",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ecfectus/container"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ecfectus/config"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ecfectus/router"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ecfectus/middleware-runner"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ecfectus/framework"
        }
    ],
    "require": {
        "ecfectus/framework": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "fabpot/php-cs-fixer": "~1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    }
}
